{
  "dashboard": {
    "title": "Indicator System V2 Performance",
    "tags": ["indicators", "performance", "v2"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Total Requests (Throughput)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(indicator_requests_total[1m]))",
            "legendFormat": "Requests/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(indicator_requests_total{status='error'}[1m])) / sum(rate(indicator_requests_total[1m])) * 100",
            "legendFormat": "Error %",
            "valueHandler": "percent"
          }
        ]
      },
      {
        "id": 3,
        "title": "Data Alignment Errors",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(indicator_alignment_errors_total[1h]))",
            "legendFormat": "Errors (1h)"
          }
        ]
      },
      {
        "id": 4,
        "title": "Streaming Latency (P99)",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(indicator_streaming_latency_seconds_bucket[1m])) by (le)) * 1000000",
            "legendFormat": "Âµs"
          }
        ]
      },
      {
        "id": 5,
        "title": "Batch Calculation Latency (P95) by Indicator",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(indicator_batch_latency_seconds_bucket[5m])) by (le, indicator_id))",
            "legendFormat": "{{indicator_id}}"
          }
        ]
      },
      {
        "id": 6,
        "title": "Streaming Update Latency (P95) by Indicator",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(indicator_streaming_latency_seconds_bucket[5m])) by (le, indicator_id)) * 1000",
            "legendFormat": "{{indicator_id}} (ms)"
          }
        ]
      },
      {
        "id": 7,
        "title": "Requests by Mode",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "sum(indicator_requests_total) by (mode)",
            "legendFormat": "{{mode}}"
          }
        ]
      },
      {
        "id": 8,
        "title": "Requests by Backend",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12},
        "targets": [
          {
            "expr": "sum(indicator_requests_total{mode='batch'}) by (backend)",
            "legendFormat": "{{backend}}"
          }
        ]
      },
      {
        "id": 9,
        "title": "Top 10 High Latency Indicators",
        "type": "table",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 12},
        "targets": [
          {
            "expr": "topk(10, sum(rate(indicator_batch_latency_seconds_sum[1h])) by (indicator_id) / sum(rate(indicator_batch_latency_seconds_count[1h])) by (indicator_id))",
            "legendFormat": "{{indicator_id}}"
          }
        ]
      }
    ]
  }
}
