{
  "name": "mystocks-web-frontend",
  "version": "1.0.0",
  "description": "MyStocks Web Management Frontend",
  "scripts": {
    "dev": "npm run generate-types && vite",
    "dev:no-types": "vite",
    "build": "npm run generate-types && vue-tsc --noEmit && vite build",
    "build:no-types": "vite build",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "generate-types": "python ../../scripts/generate_frontend_types.py",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@playwright/test": "^1.56.1",
    "axios": "^1.7.0",
    "dayjs": "^1.11.0",
    "echarts": "^5.5.0",
    "element-plus": "^2.13.0",
    "klinecharts": "^9.8.12",
    "lodash-es": "^4.17.0",
    "pinia": "^2.2.0",
    "technicalindicators": "^3.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/eslint-config-prettier": "^10.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^9.25.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.3.0",
    "sass": "^1.77.0",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.18.0",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.4.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^1.8.27"
  },
  "keywords": [
    "vue",
    "element-plus",
    "stock",
    "trading",
    "charts"
  ],
  "author": "MyStocks Team",
  "license": "MIT"
}
