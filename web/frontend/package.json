{
  "name": "mystocks-web-frontend",
  "version": "1.0.0",
  "description": "MyStocks Web Management Frontend",
  "scripts": {
    "dev": "npm run generate-types && vite",
    "dev:mock": "cp .env.mock .env && npm run generate-types && vite",
    "dev:real": "cp .env.real .env && npm run generate-types && vite",
    "dev:no-types": "vite",
    "build": "npm run generate-types && (vue-tsc --noEmit || true) && vite build",
    "build:no-types": "vite build",
    "type-check": "vue-tsc --noEmit",
    "type:validate": "node scripts/validate-types.js",
    "type:check:conflicts": "node scripts/check-type-conflicts.js",
    "type:usage": "node scripts/generate-type-usage.js",
    "type:build": "npm run generate-types && npm run type:validate && npm run type-check",
    "preview": "vite preview",
    "generate-types": "python ../../scripts/generate_frontend_types.py",
    "generate-page-config": "node ../../scripts/dev/tools/generate-page-config.js",
    "validate-page-config": "node ../../scripts/hooks/check-page-config.mjs --fail",
    "lint": "eslint . --fix",
    "lint:artdeco": "node scripts/check-artdeco-tokens.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:comprehensive": "node run-comprehensive-e2e.js",
    "test:e2e:validate": "node validate-e2e-setup.js",
    "test:pm2": "playwright test pm2-deployment.test.ts",
    "test:design-token": "playwright test design-token.test.ts",
    "test:bloomberg": "playwright test bloomberg-style.test.ts",
    "test:stock-colors": "playwright test stock-colors.test.ts",
    "test:mobile-cleanup": "playwright test mobile-cleanup.test.ts",
    "test:phase3": "playwright test pm2-deployment.test.ts design-token.test.ts bloomberg-style.test.ts stock-colors.test.ts mobile-cleanup.test.ts",
    "pm2:start": "./start.sh",
    "pm2:stop": "./stop.sh",
    "pm2:restart": "pm2 reload ecosystem.config.js --env production",
    "pm2:logs": "pm2 logs mystocks-frontend --lines 50",
    "pm2:status": "pm2 list"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@element-plus/icons-vue": "^2.3.2",
    "@playwright/test": "^1.56.1",
    "ant-design-vue": "^4.2.6",
    "dayjs": "^1.11.0",
    "echarts": "^5.5.0",
    "element-plus": "^2.13.0",
    "klinecharts": "^9.8.12",
    "lodash-es": "^4.17.0",
    "pinia": "^2.2.0",
    "technicalindicators": "^3.1.0",
    "vue": "^3.4.0",
    "vue-grid-layout": "^2.4.0",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.3.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/test-utils": "^2.4.6",
    "axios": "^1.13.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^9.25.0",
    "fuse.js": "^7.1.0",
    "glob": "^13.0.2",
    "happy-dom": "^20.0.11",
    "playwright": "^1.57.0",
    "postcss-html": "^1.8.1",
    "prettier": "^3.3.0",
    "puppeteer": "^24.36.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.77.0",
    "stylelint": "^17.2.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-scss": "^7.0.0",
    "terser": "^5.44.1",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.4.0",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^1.8.27"
  },
  "keywords": [
    "vue",
    "element-plus",
    "stock",
    "trading",
    "charts"
  ],
  "author": "MyStocks Team",
  "license": "MIT"
}
